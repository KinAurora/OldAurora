local TweenService = game:GetService("TweenService")

-- Create a function to display the neon watermark
local function showNeonWaterMark()
    local NeonWaterMark = Instance.new("ScreenGui")
    local TextLabel = Instance.new("TextLabel")
    local UITextSizeConstraint = Instance.new("UITextSizeConstraint")

    -- Setup the ScreenGui
    NeonWaterMark.Name = "NeonWaterMark"
    NeonWaterMark.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
    NeonWaterMark.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    NeonWaterMark.ResetOnSpawn = false

    -- Setup the TextLabel
    TextLabel.Parent = NeonWaterMark
    TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    TextLabel.BackgroundTransparency = 1.000
    TextLabel.BorderSizePixel = 0
    TextLabel.Position = UDim2.new(0, 0, 0, 0)
    TextLabel.Size = UDim2.new(0.2, 0, 0.15, 0)
    
    -- Change font to a bolder one
    TextLabel.Font = Enum.Font.SourceSansBold
    TextLabel.Text = "Aurora V4 BedWars"
    TextLabel.TextColor3 = Color3.fromRGB(0, 191, 255)
    TextLabel.TextScaled = false  -- Disable TextScaled
    TextLabel.TextSize = 42  -- Set fixed text size
    TextLabel.TextWrapped = true
    TextLabel.TextXAlignment = Enum.TextXAlignment.Left

    -- Add UITextSizeConstraint for dynamic text sizing
    UITextSizeConstraint.MaxTextSize = 42
    UITextSizeConstraint.MinTextSize = 42
    UITextSizeConstraint.Parent = TextLabel

    -- Add a stroke to make the text appear bolder with a black outline
    TextLabel.TextStrokeTransparency = 0.5  -- Stroke transparency (0 = opaque, 1 = no stroke)
    TextLabel.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)  -- Black stroke color

    -- Tween for moving position
    local goal = {Position = UDim2.new(0.85, 0, 0, -70)}
    local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
    local tween = TweenService:Create(TextLabel, tweenInfo, goal)
    tween:Play()

    -- Create the second text element with the same font and Y-axis position
    local CustomWaterMark = Instance.new("ScreenGui")
    local CustomTextLabel = Instance.new("TextLabel")
    local CustomUITextSizeConstraint = Instance.new("UITextSizeConstraint")

    CustomWaterMark.Name = "CustomWaterMark"
    CustomWaterMark.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
    CustomWaterMark.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    CustomWaterMark.ResetOnSpawn = false

    CustomTextLabel.Parent = CustomWaterMark
    CustomTextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    CustomTextLabel.BackgroundTransparency = 1.000
    CustomTextLabel.BorderSizePixel = 0
    CustomTextLabel.Position = UDim2.new(0, 0, 0, -40)  -- Align Y position to match the first TextLabel
    CustomTextLabel.Size = UDim2.new(0.2, 0, 0.15, 0)

    -- Match the font to the first TextLabel
    CustomTextLabel.Font = Enum.Font.SourceSansBold
    CustomTextLabel.Text = "discord.gg/FubS26Fa29"
    CustomTextLabel.TextColor3 = Color3.fromRGB(0, 191, 255)
    CustomTextLabel.TextScaled = false  -- Disable TextScaled
    CustomTextLabel.TextSize = 42  -- Set fixed text size
    CustomTextLabel.TextWrapped = true
    CustomTextLabel.TextXAlignment = Enum.TextXAlignment.Left
    CustomTextLabel.TextStrokeTransparency = 0.5  -- Stroke transparency (0 = opaque, 1 = no stroke)
    CustomTextLabel.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)  -- Black stroke color

    -- Add UITextSizeConstraint for dynamic text sizing
    CustomUITextSizeConstraint.MaxTextSize = 42
    CustomUITextSizeConstraint.MinTextSize = 42
    CustomUITextSizeConstraint.Parent = CustomTextLabel
end

-- Call the function to display the watermark
showNeonWaterMark()
